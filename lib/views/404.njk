{% extends defaultRigLayout or "template.njk" %}

{% set title = "Page not found" %}

{% block content %}
  <div class="nhsuk-grid-row">
    <div class="nhsuk-grid-column-two-thirds">
      <h1 class="nhsuk-heading-xl">{{ title }}</h1>
      <p class="nhsuk-body">If you entered a web address, check it is correct.</p>
      <pre class="nhsuk-!-margin-top-8">Status code: 404</pre>
    </div>
  </div>
{% endblock %}
