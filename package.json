{
  "name": "nhsuk-prototype-rig",
  "version": "0.0.7",
  "description": "Rapidly create prototypes of NHS.UK services",
  "keywords": [
    "nhsuk",
    "design system",
    "prototyping"
  ],
  "bugs": {
    "url": "https://github.com/x-govuk/nhsuk-prototype-rig/issues"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "bin": {
    "nhsuk-prototype-rig": "./bin.js"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/x-govuk/nhsuk-prototype-rig.git"
  },
  "scripts": {
    "build-assets": "rollup --config --silent",
    "watch-assets": "rollup --config --silent --watch",
    "serve": "nhsuk-prototype-rig",
    "start": "npm-run-all --serial build-assets serve",
    "dev": "npm-run-all --parallel watch-assets serve",
    "lint": "standard && stylelint 'app/**/*.scss'",
    "test": "ava",
    "coverage": "c8 ava"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@x-govuk/govuk-prototype-components": "^2.0.2",
    "@x-govuk/govuk-prototype-filters": "^1.1.0",
    "browser-sync": "^2.27.8",
    "client-sessions": "^0.8.0",
    "cookie-parser": "^1.4.6",
    "cosmiconfig": "^8.3.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^7.1.0",
    "express-session": "^1.13.0",
    "govuk-frontend": "^4.5.0",
    "govuk-prototype-wizard": "^0.2.0",
    "inquirer": "^9.0.0",
    "lodash": "^4.17.21",
    "mock-req-res": "^1.2.0",
    "nhsuk-decorated-components": "^0.0.5",
    "nhsuk-frontend": "^8.0.0",
    "nhsuk-prototype-rig": "file:./",
    "nodemon": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.1",
    "portscanner": "^2.2.0",
    "rollup": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.49.9",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "ava": "^5.0.0",
    "c8": "^8.0.0",
    "sinon": "^16.1.0",
    "standard": "^17.0.0",
    "stylelint": "^15.0.0",
    "stylelint-config-gds": "^1.0.0",
    "supertest": "^6.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "c8": {
    "exclude": [
      "app",
      "tests"
    ],
    "reporter": [
      "text",
      "lcovonly"
    ]
  },
  "engines": {
    "node": "^18"
  },
  "type": "module",
  "stylelint": {
    "extends": "stylelint-config-gds/scss"
  }
}
